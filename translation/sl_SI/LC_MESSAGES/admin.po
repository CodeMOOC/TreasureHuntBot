# Welcome message when registering new game for event
msgid "creation_welcome"
msgstr "Dobrodošli - pomagali vam bomo pri organizaciji igre."

# Confirm game registration
msgid "creation_welcome_confirm"
msgstr "Potrjujemo registracijo vaše igre."

# Abort game registration
msgid "creation_welcome_abort"
msgstr "Registracija igre je bila prekinjena."

# Name request for game
msgid "creation_set_name"
msgstr "Določite ime igre."

# Telegram channel request for game
msgid "creation_set_channel"
msgstr "Določite ime kanala na Telegramu, v katerem bo potekala igra."

# Skip button for telegram channel request
msgid "creation_set_channel_skip"
msgstr "Preskoči kreiranje kanala v Telegramu."

# Attempting to write on public channel, please wait
msgid "creation_set_channel_write_attempt"
msgstr "Poskušamo pisati v Telegramov kanal, prosimo počakajte."

# Confirmation of public channel
msgid "creation_set_channel_confirm"
msgstr "Kanal za igro je pripravljen."

# Failed to write on public channel
msgid "creation_set_channel_failure"
msgstr "Napaka pri pisanju v Telegramov kanal."

# Prompt user for new Telegram channel or to retry writing
msgid "creation_set_channel_again"
msgstr "Ustvarite nov kanal ali pa poskusite ponovno pisati v obstoječega. "

# Retry button to write to public Telegram channel
msgid "creation_set_channel_retry"
msgstr "Ponovno zapišite v kanal."

# Prompt user for image censorship on public channel
msgid "creation_set_channel_censorship"
msgstr "Ali dovolite javno objavo fotografij v tem kanalu?"

# Enable censorship button
msgid "creation_set_channel_censorship_enable"
msgstr "Skrij fotografije, da ne bodo javno dostopne"

# Disable censorship button
msgid "creation_set_channel_censorship_disable"
msgstr "Dovoli javno objavo fotografij med igro."

# Prompt for contact e-mail
msgid "creation_set_email"
msgstr "Vpišite svoj email"

# Confirm e-mail registration
msgid "creation_set_email_confirm"
msgstr "Potrdite registracijo prek sporočila, ki smo vam ga poslali na email"

# E-mail supplied is not valid
msgid "creation_set_email_wrong"
msgstr "Vpisan email ni veljaven"

# Beginning of location registrations
msgid "creation_location_begin"
msgstr "Začnite z vnosom lokacij"

# Prompt for first location (start position)
msgid "creation_location_start"
msgstr "Vnesite začetno lokacijo - tukaj bo start igre"

# Prompt for last location
msgid "creation_location_end"
msgstr "Vnesite končno lokacijo - tukaj bo cilj igre"

# Prompt for next location, if any (%COUNT% total registered for game, %MINIMUM% minimum locations for game, %AVGDIST% average distance of locations, %MINAVGDIST% minimum average distance of locations)
msgid "creation_location_next"
msgstr "Vnesite naslednjo lokacijo"

# Prompt for next location if number of locations reached but not minimum average distance (%COUNT% total registered for game, %MINIMUM% minimum locations for game, %AVGDIST% average distance of locations, %MINAVGDIST% minimum average distance of locations)
msgid "creation_location_next_insufficient_avg_distance"
msgstr "Razdalja med to in prejšnjo lokacijo je premajhna."

# Stop button for location insertion, appears if minimum number of locations is reached and other constraints are met
msgid "creation_location_next_stop"
msgstr "Ok, tukaj lahko ustvarite novo lokacijo"

# Prompt for internal description of location
msgid "creation_location_set_name"
msgstr "Vpišite podatke o lokaciji"

# Confirm successful creation
msgid "creation_completed"
msgstr "Vse ok!"

# Generic failure, something went wrong, start again please
msgid "failure"
msgstr "Prišlo je do napake, prosim, poskusite ponovno."

